# Porta do servidor
PORT=3000

# Banco de dados
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=disparador_webhooks

# Redis
REDIS_HOST=localhost
